GEMINI_API_KEY=your_gemini_key_here
GEMINI_MODEL=gemini-1.5-flash-latest
# If you want to use Groq instead, set GROQ_API_KEY and (optionally) GROQ_MODEL
# GROQ_API_KEY=your_groq_key_here
# GROQ_MODEL=llama-3.3-70b-versatile
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/customer

# Safety / performance
NL2SQL_STATEMENT_TIMEOUT_MS=8000
NL2SQL_MAX_ROWS=200
NL2SQL_MEMORY_USER_TURNS=5
NL2SQL_MAX_SQL_STATEMENTS=4
